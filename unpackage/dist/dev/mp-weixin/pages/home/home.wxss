/* 水平间距 */
.home_page {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
.home_page .swiper_item {
  height: 300rpx;
}
.home_page .swiper_item .img {
  width: 100%;
  height: 100%;
}
.home_page .list_type {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
}
.home_page .list_type .list_item {
  flex: 1;
  text-align: center;
  margin: 15rpx 0;
}
.home_page .list_type .list_item .listImg {
  width: 140rpx;
  height: 140rpx;
}
.home_page .floor-title {
  height: 60rpx;
  width: 100%;
  display: flex;
}
.home_page .rigth-img-box {
  display: flex;
  flex-wrap: wrap;
  padding-left: 10rpx;
}
.home_page .rigth-img-box .floorImgitems {
  padding-left: 10rpx;
}
.home_page .floor-img-box {
  display: flex;
}

